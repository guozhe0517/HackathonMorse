        <userprofile type="value" isBundle="false" isAutoGenerateModelKey="false"/>
        <userchatrooms type="reference-key">
            <userchatroom type="value" isBundle="true" isAutoGenerateModelKey="false" refer="" />
        </userchatrooms>
        <friends type="reference-key">
            <friend type="value" isBundle="true" isAutoGenerateModelKey="false" refer=""/>
        </friends>
    </user>
</users>
